if getgenv().Key then
    -- ตรวจสอบ key ใน Lua script
    local phpScriptUrl = "https://raw.githubusercontent.com/Thanakrit001/UNIHUBKEY/main/Key"
    local check = game:HttpGet(phpScriptUrl)

    if check == "Key" then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Thanakrit001/UNI-X-HUB/main/UNI%20X%20HUB"))()
    end
end
