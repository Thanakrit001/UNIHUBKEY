-- ตรวจสอบว่า _G.Key ถูกกำหนดค่าหรือไม่
if _G.Key then
    -- กำหนด URL ที่ใช้ตรวจสอบ key
    local check = "https://raw.githubusercontent.com/Thanakrit001/UNIHUBKEY/main/Key?key=" .. _G.Key
    
    -- ดึงข้อมูลจากเซิร์ฟเวอร์
    local result = game:HttpGet(check)

    -- ตรวจสอบผลลัพธ์และทำการโหลดสคริปต์หาก key ถูกต้อง
    if result and result == "Whitelisted" then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Thanakrit001/UNI-X-HUB/main/UNI%20X%20HUB"))()
    else
        game.Players.LocalPlayer:Kick("Invalid Key! Please Rejoin And Try Again.")
    end
else
    warn("Key not defined!")
end
